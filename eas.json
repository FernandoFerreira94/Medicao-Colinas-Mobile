{
  "cli": {
    "version": ">= 16.24.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_USE_LEGACY_NPM_PEER_DEPS": "true"
      }
    },
    "apk-test": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "env": {
        "EXPO_USE_LEGACY_NPM_PEER_DEPS": "true",
        "npm_config_legacy_peer_deps": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_USE_LEGACY_NPM_PEER_DEPS": "true"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_USE_LEGACY_NPM_PEER_DEPS": "true"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
